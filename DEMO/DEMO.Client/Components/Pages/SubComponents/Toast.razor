﻿<div id="liveToast" class="toast show" role="alert" aria-live="assertive" aria-atomic="true">
    <div class="toast-header text-muted" style="border: none; @Notification.HeaderStyle">
        <strong class="me-auto">@Notification?.Title</strong>
    </div>
    <div class="toast-body text-muted" style="background-color: #0d2940 !important">
        @Notification?.Message
    </div>
</div>